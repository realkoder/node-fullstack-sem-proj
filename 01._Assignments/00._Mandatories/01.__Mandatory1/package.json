{
  "name": "nodejs-fullstack-documentation",
  "version": "1.0.0",
  "main": "app.mjs",
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "module",
  "scripts": {
    "build:css": "postcss public/assets/css/tailwind.css -o public/assets/css/styles.css",
